FACT rock_5 3 0 0;
FACT waterFound 0;	

Plan: {
NAME:
		"Find water"
GOAL:
		ACHIEVE rocks_delivered;
BODY:
		RETRIEVE position $x $y;
		RETRIEVE playArea $b $c;
		RETRIEVE direction $d;
		WHEN : TEST(== $x 0)
		{	
			WHEN : TEST(== $y 0)
			{
				EXECUTE println "Currently at base   ";
			};		
		};
		RETRIEVE waterFound $a;
		WHILE : TEST(!= $a 2)
		{
			WHEN : TEST(== $d 1)
			{
				PERFORM east;
			};
			WHEN : TEST(== $d 2)
			{				
				PERFORM north;
			};
			WHEN : TEST(== $d 3)
			{
				PERFORM west;
			};
			WHEN : TEST(== $d 5)
			{
				PERFORM east1;
			};
			WHEN : TEST(== $d 4)
			{			
				PERFORM north1;
			};
			WHEN : TEST(== $d 7)
			{
				PERFORM west1;
			};
			WHEN : TEST(== $d 11)
			{
				PERFORM west2;
			};
			WHEN : TEST(== $d 6)
			{			
				PERFORM north2;
			};
			WHEN : TEST(== $d 8)
			{			
				PERFORM north3;
			};
			WHEN : TEST(== $d 10)
			{			
				PERFORM north4;
			};
			WHEN : TEST(== $d 12)
			{			
				PERFORM north5;
			};
			WHEN : TEST(== $d 9)
			{
				PERFORM east2;
			};
			WHEN : TEST(== $d 13)
			{
				PERFORM east3;
			};
		};
}

